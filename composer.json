{
  "name": "pvsaintpe/yii2-search",
  "description": "Package",
  "keywords": [
    "yii2",
    "gii",
    "code generator"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Veselov",
      "email": "pvsaintpe@icloud.com"
    }
  ],
  "require": {
    "pvsaintpe/yii2-grid": "*",
    "pvsaintpe/yii2-gii-plus": "2.*"
  },
  "autoload": {
    "psr-4": {
      "pvsaintpe\\search\\": ""
    }
  },
  "extra": {
    "bootstrap": "pvsaintpe\\search\\Bootstrap",
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
